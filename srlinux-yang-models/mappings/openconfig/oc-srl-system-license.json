{
  "mapping": [
    {
      "oc": "/system/license",
      "srlinux": ""
    },
    {
      "oc": "/system/license/licenses",
      "srlinux": ""
    },
    {
      "oc": "/system/license/licenses/license",
      "srlinux": "/system/srl_nokia-license:license"
    },
    {
      "oc": "/system/license/licenses/license/license-id",
      "srlinux": "/system/srl_nokia-license:license/srl_nokia-license:id"
    },
    {
      "oc": "/system/license/licenses/license/{config,state}",
      "srlinux": ""
    },
    {
      "oc": "/system/license/licenses/license/{config,state}/license-id",
      "srlinux": ""
    },
    {
      "oc": "/system/license/licenses/license/{config,state}/license-data",
      "srlinux": "/system/srl_nokia-license:license/srl_nokia-license:data"
    },
    {
      "oc": "/system/license/licenses/license/{config,state}/active",
      "srlinux": "/system/srl_nokia-license:license/srl_nokia-license:admin-state",
      "special-handling": "bool-to-admin-state" 
    },
    {
      "oc": "/system/license/licenses/license/state/description",
      "srlinux": "/system/srl_nokia-license:license/srl_nokia-license:description"
    },
    {
      "oc": "/system/license/licenses/license/state/issue-date",
      "srlinux": "/system/srl_nokia-license:license/srl_nokia-license:issued-date",
      "special-handling": "epoch-to-date-time"
    },
    {
      "oc": "/system/license/licenses/license/state/expiration-date",
      "srlinux": "/system/srl_nokia-license:license/srl_nokia-license:expiration-date",
      "special-handling": "epoch-to-date-time"
    },
    {
      "oc": "/system/license/licenses/license/state/in-use",
      "srlinux": "/system/srl_nokia-license:license/srl_nokia-license:in-use"
    },
    {
      "oc": "/system/license/licenses/license/state/expired",
      "srlinux": "/system/srl_nokia-license:license/srl_nokia-license:expired"
    },
    {
      "oc": "/system/license/licenses/license/state/valid",
      "srlinux": "/system/srl_nokia-license:license/srl_nokia-license:valid"
    }
  ]
}

