  feature mirroring {
    description
      "Support for mirroring (SPAN/ERSPAN)";
  }

  feature mld {
    if-feature "future-0-0";
    description
      "Support for MLD";
  }

  feature mpls {
    description
      "Support for MPLS";
  }

  feature mpls-ndk {
    description
      "Support for NDK static MPLS";
  }

  feature oam {
    description
      "Support for OAM";
  }

  feature oc-extracted-cpm-traffic-stats {
    description
      "Support for OpenConfig extracted CPM traffic statistics";
  }

  feature openconfig {
    description
      "Support for OpenConfig";
  }

  feature p4rt {
    if-feature "future-0-0";
    description
      "Support for P4Runtime";
  }

  feature pim {
    description
      "Support for PIM";
  }

