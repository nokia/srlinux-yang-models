  grouping mpls-tc-rewrite {
    list mpls-traffic-class-policy {
      if-feature "srl-feat:jericho2 or srl-feat:jericho2cp or srl-feat:future-0-0";
      key "name";
      leaf name {
        type srl_nokia-comm:name;
